#!/bin/bash

# Get the current directory
current_directory=$(pwd)

# Get a list of all files in the current directory
files=$(ls -t "$current_directory")

# Display the 10 newest files
echo "10 Newest Files:"
count=0
for file in $files; do
    if [ -f "$file" ]; then
        echo "$file"
        count=$((count + 1))
    fi

    # Exit the loop after displaying 10 files
    if [ $count -eq 10 ]; then
        break
    fi
done

